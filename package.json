  "scripts": {
    "test": "npm run lint && nyc ava",
    "coverage": "nyc report && codecov",
    "lint": "npm run lint-js && npm run lint-webext",
    "lint-js": "eslint test/ src/",
    "lint-webext": "webpack --hide-modules && web-ext lint -s webextension/",
    "build": "webpack && web-ext build -s webextension/",
    "start": "webpack && web-ext run -s webextension/"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-class-properties"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
