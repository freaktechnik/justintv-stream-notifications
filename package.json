{
  "homepage": "http://streamnotifier.ch/",
  "author": "freaktechnik (https://humanoids.be)",
  "name": "justintv-stream-notifications",
  "version": "3.11.1",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/freaktechnik/justintv-stream-notifications.git"
  },
  "bugs": {
    "url": "https://github.com/freaktechnik/justintv-stream-notifications/issues"
  },
  "engines": {
    "node": ">= 11.14.0"
  },
  "files": [
    "webextension/assets/images/*",
    "webextension/manifest.json",
    "src/*",
    "scripts/*",
    "_locales/*/messages.json",
    ".tx/config",
    "webpack.config.js",
    "package-lock.json",
    "!*.eslintrc.json",
    "!webextension/assets/images/open-iconic.min.svg"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-key-handler": "^1.1.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/traverse": "^7.1.6",
    "@babel/types": "^7.2.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@freaktechnik/eslint-config-babel": "^7.0.0",
    "@freaktechnik/eslint-config-base": "^7.0.0",
    "@freaktechnik/eslint-config-extension": "^7.0.0",
    "@freaktechnik/eslint-config-node": "^7.0.0",
    "@freaktechnik/eslint-config-react": "^7.0.0",
    "@freaktechnik/eslint-config-test": "^7.0.0",
    "@freaktechnik/eslint-config-webpack": "^7.0.0",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/exec": "^3.3.7",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.5.3",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "ajv": "^6.10.2",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browser-env": "^3.2.6",
    "codecov": "^3.6.1",
    "css-loader": "^3.2.0",
    "eclint": "^2.8.1",
    "element-dataset": "^2.2.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "event-target-shim": "^5.0.1",
    "fake-indexeddb": "^2.1.1",
    "fetch-headers": "^2.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "husky": "^3.0.9",
    "mini-css-extract-plugin": "^0.8.0",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.24",
    "simple-git": "^1.126.0",
    "sinon": "^7.5.0",
    "sinon-chrome": "^3.0.1",
    "stream-to-promise": "^2.2.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "transifex-loader": "^5.0.2",
    "web-ext": "^3.2.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-deep-scope-plugin": "^1.6.2"
  },
  "scripts": {
    "test:ava": "NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run test:ava",
    "coverage": "nyc report && codecov",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:css && npm run lint:webext && npm run lint:ec",
    "lint:json": "eslint -c src/.eslintrc.json --ext json webextension/manifest.json",
    "lint:js": "eslint test/* src/* scripts/* --ext js,jsx,json",
    "lint:webext": "NODE_ENV=test webpack --hide-modules --mode development && web-ext lint",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:ec": "eclint check $(git ls-files | grep -vEe \"_locales/([^e][^n]|e.{2,})/.*\" -e=\"*/snapshots/*\")",
    "build": "NODE_ENV=production webpack --mode production && web-ext build && npm pack",
    "start": "NODE_ENV=development webpack --mode development && web-ext run --no-reload",
    "ci-init": "node ./scripts/setup-travis.js",
    "release": "semantic-release"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false,
        "presets": [
          "@babel/preset-react"
        ]
      }
    },
    "require": [
      "./test/helpers/register.js",
      "./test/helpers/setup-browser-env.js"
    ],
    "helpers": [
      "test/helpers/**/*"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "alias": {
            "open-iconic/sprite/open-iconic.min.svg": "./test/helpers/assets/open-iconic.js"
          }
        }
      ],
      "istanbul"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".jsx"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {
      "indentation": 4,
      "color-named": "always-where-possible",
      "plugin/no-unsupported-browser-features": [
        true,
        {
          "severity": "warning",
          "ignore": [
            "css3-cursors"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "last 1 Firefox versions",
    "last 1 FirefoxAndroid versions"
  ],
  "webExt": {
    "sourceDir": "webextension/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "ci": false,
    "repositoryUrl": "git@github.com:freaktechnik/justintv-stream-notifications.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "node ./scripts/bump-manifest.js && npm run build",
          "successCmd": "node ./scripts/build-website-changelog.js"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "_locales/**/messages.json",
            "!_locales/en/messages.json",
            "webextension/manifest.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "failComment": false,
          "failTitle": false,
          "releasedLabels": false
        }
      ]
    ]
  },
  "private": true
}
